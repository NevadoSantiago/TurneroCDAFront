{
  "expo": {
    "name": "turnero",
    "slug": "turnero",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/IMAGENCDA.png",
      "resizeMode": "contain",
      "backgroundColor": "#0473c3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.cda.turnero",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.cda.turnero",
      "versionCode": 1
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "scss", "sass"]
    }
  }
}
